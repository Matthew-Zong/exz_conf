
####################################
# Proxy               
####################################
Host *.qunar.com               
ServerAliveInterval 300                                                                                                      
ControlMaster auto             
ControlPath ~/.ssh/master-%r@%h:%p 
StrictHostKeyChecking no       
                     
Host *               
    ControlMaster auto         
    ControlPath /tmp/ssh-%r@%h:%p  
                     
Host *.qunar.com               
    Port 22          
    ForwardAgent yes           
         
Host *.cn1.qunar.com           
    User zhen.pei
    ProxyCommand ssh l-rtools1.ops.cn1.qunar.com -W %h:%p -q 2> /dev/null
                     
Host *.cn5.qunar.com           
    User zhen.pei            
    ProxyCommand ssh l-rtools1.ops.cn5.qunar.com -W %h:%p -q 2> /dev/null
                     
Host *.cn6.qunar.com           
    User zhen.pei            
    ProxyCommand ssh l-rtools1.ops.cn6.qunar.com -W %h:%p -q 2> /dev/null
####################################


